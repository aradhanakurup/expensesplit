{
  "expo": {
    "name": "ExpenseSplit",
    "slug": "expensesplit",
    "version": "1.0.0",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTabletMode": true,
      "bundleIdentifier": "com.expensesplit.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.expensesplit.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow ExpenseSplit to access your camera"
        }
      ]
    ],
    "permissions": [
      "camera",
      "photo"
    ]
  }
}
